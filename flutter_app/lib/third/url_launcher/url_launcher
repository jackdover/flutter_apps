https://pub.dev/packages/url_launcher/install

url_launcher: ^5.7.10

import 'package:url_launcher/url_launcher.dart';








_launchURL() async {
  const url = 'https://flutter.dev';
  if (await canLaunch(url)) {
    await launch(url);
  } else {
    throw 'Could not launch $url';
  }
}













